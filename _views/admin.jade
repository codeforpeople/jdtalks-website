doctype html
html
  head
    title= title
    link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Open+Sans+Condensed:300,600&subset=latin,latin-ext', type='text/css')
    link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Open+Sans:300,600&subset=latin,latin-ext', type='text/css')
    link(rel='stylesheet', href='/css/bootstrap.css')
    link(rel='stylesheet', href='/css/style.css')
    link(rel='stylesheet', href='/css/admin.css')
  body
    .container#admin
      .row#admin
        .col-md-4.col-md-offset-5
          h1(style='font-weight:bold;padding-bottom:20px;') Admin Panel
        .col-md-4.col-md-offset-4.centering#admin
          form.form-horizontal
            .form-group
              label(for='inputEmail3',class='col-sm-2 control-label') Username
              .col-sm-10
                input(type='text',class='form-control',id='inputEmail3',placeholder='Username')
            .form-group
              label(for='inputPassword3',class='col-sm-2 control-label') Password
              .col-sm-10
                input(type='password',class='form-control',id='inputPassword3',placeholder='Password')
            .form-group
              .col-sm-offset-9.col-sm-3
                button(type='submit',class='btn btn-success') Sign In
    script(src='/js/jquery.js')
    script(src='/js/bootstrap.js')
    script(src='/js/ga.js')
